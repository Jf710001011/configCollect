{
  "files.associations": {
    "*.json": "jsonc"
  },
  "defaultFontFamily": {
    "standard": null,
    "serif": null,
    "sansSerif": null,
    "monospace": null
  },
  "autoHideMenuBar": false,
  "searchService": [
    [
      "Search with Google",
      "https://google.com/search?q=%s"
    ]
  ],
  "keyBinding": {
    "Inline Code": "ctrl+`",
    "Increase Heading Level": "ctrl+shift+]",
    "Decrease Heading Level": "ctrl+shift+[",
    "Quote": "ctrl+shift+q",
    "Ordered List": "ctrl+shift+o",
    "Unordered List": "ctrl+shift+u",
    "YAML Front Matter": "ctrl+0",
    "Hyperlink": "ctrl+l",
    "Strikethrough": "ctrl+d",
    "Code Fences": "alt+`"
  },
  "monocolorEmoji": false,
  "maxFetchCountOnFileList": 500,
  "flags": []
}
